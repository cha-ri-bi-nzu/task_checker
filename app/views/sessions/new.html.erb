<h3>Log in</h3>
<br>
<div>
  <p>
    <%= form_with(local: true, url: sessions_path, scope: :session) do |f| %>
      　<%= f.label :email %>
      <%= f.email_field :email %>　
      
      <%= f.label :password %>
      <%= f.password_field :password %>　

      <%= f.submit "Log in" %>
    <% end %>
  </p>
</div>